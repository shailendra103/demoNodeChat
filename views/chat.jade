extend layout

block scripts
  script(src="https://code.jquery.com/jquery-3.1.1.min.js")
  script(src="/socket.io/socket.io.js")

block content
  .container-fluid
      .row-fluid
        .span2
          h1 Online users
          ul.nav.nav-stacked.nav-tabs#users

        .span10.chat-wrap
          #thread
          .in-putter.span8
            form#sendmessage
              input(type="text")#message
              input(type="hidden", id="name", value="shail")
              button.btn.btn-large#send.btn-danger
                i.icon-bolt
                |  Send
          script(src="sock.js")
